function [avg avg_1] = cdf()
index = 0;
data=[];
for th = [1 3 7]
    index = index +1;
    [a b c d e f] = changeDevice(th);
    avg = [1:20]*0;
    for i=1:length(b)
        x = b{i};
        for j=1:length(x)
        avg(j) = avg(j) + x(j);
        end
    end
    avg = avg/8;
    avg_1 = [1:20]*0;
    for i=1:length(c)
        x = c{i};
        for j=1:length(x)
        avg_1(j) = avg_1(j) + x(j);
        end
    end
    avg_1 = avg_1 /8;
    cdfplot(avg(3:end));
    hold on;
    z=sum(avg(2:end));
    c=mean(f);
    x=sum(avg_1(2:end));
    data(index,1) = c;
    data(index,2) = z-c;
    data(index,3) = x-c;
end
bar(data,'stack')
%cdfplot(avg_1(3:end));
end